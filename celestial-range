        });

        if ($(window).width() < 768) {
            function OnloadGoogleRecaptcha() {
                CELESTIALRANGEPAGE.scaleRecaptcha();
            }
        }

        function captureGRecaptchaResponse(token) {
            $("#googleRecaptchaToken").val(token);
        }
        $('#float-btn,#float-btn-mbl').click(function() {
            $('html, body').animate({
                scrollTop: $("section.celestial-form").offset().top
            }, 1000);

        });


        $('#float-btn,#float-btn-mbl').hide();
        $(window).scroll(function() {
            var showFloatBtn = $(this).scrollTop();
            var heightToScroll = $('#vid').height();
            if (showFloatBtn > heightToScroll) {
                $('#float-btn,#float-btn-mbl').fadeIn();
            } else {
                $('#float-btn,#float-btn-mbl').fadeOut();
            }
        });</script> <!-- endbuild -->  